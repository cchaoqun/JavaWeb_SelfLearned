<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>PrintHtml</servlet-name>
        <servlet-class>com.AtGuiGu.Servlet.PrintHtml</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>PrintHtml</servlet-name>
        <url-pattern>/printHtml</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SearchStuServlet</servlet-name>
        <servlet-class>com.AtGuiGu.Servlet.SearchStuServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SearchStuServlet</servlet-name>
        <url-pattern>/searchStuServlet</url-pattern>
    </servlet-mapping>

    <listener>
        <listener-class>com.AtGuiGu.listener.ServletContextListenerImpl</listener-class>
    </listener>
</web-app>